	<refentry id="function.pow">
		<refnamediv>
			<refname>pow</refname>
			<refpurpose>거듭제곱</refpurpose>
		</refnamediv>
		<refsect1>
			<title>설명</title>
				<methodsynopsis>
					<type>float</type><methodname>pow</methodname>
					<methodparam><type>float</type><parameter>base</parameter></methodparam>
					<methodparam><type>float</type><parameter>exp</parameter></methodparam>
				</methodsynopsis>
			<para>
				<parameter>base</parameter>의 <parameter>exp</parameter>승 값을 반환한다.
    </para>
			<para>
				<function>exp</function>을 참고하라.
    </para>
		</refsect1>
	</refentry>

