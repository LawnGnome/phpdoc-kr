<!-- splitted from ./kr/functions/math.xml, last change in rev 1.3 -->
<!-- last change to 'asin' in en/ tree in rev 1.57 -->
	<refentry id="function.asin">
		<refnamediv>
			<refname>asin</refname>
			<refpurpose>아크 사인(Arc sine)</refpurpose>
		</refnamediv>
		<refsect1>
			<title>설명</title>
				<methodsynopsis>
					<type>float</type><methodname>asin</methodname>
					<methodparam><type>float</type><parameter>arg</parameter></methodparam>
				</methodsynopsis>
			<para>
				<parameter>arg</parameter>의 아크 사인(Arc sine) 라디안으로 값을 반환한다.
    </para>
			<para>
				<function>acos</function> 그리고 <function>atan</function>을 참고하라.
    </para>
		</refsect1>
	</refentry>

